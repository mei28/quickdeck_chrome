{
  "manifest_version": 3,
  "name": "Quickdeck",
  "version": "1.0",
  "description": "Extract and open SlideShare embed URL in a new tab.",
  "permissions": [
    "tabs",
    "contextMenus",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
